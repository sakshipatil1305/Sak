package com.beans;

public class Book {

	String isbn,name,publication,author;

	public Book(String isbn, String name, String publication, String author) {
		super();
		this.isbn = isbn;
		this.name = name;
		this.publication = publication;
		this.author = author;
	}

	public Book() {
		super();
		// TODO Auto-generated constructor stub
	}
	
}
