package com.pojo;

public class Student {
	String name;
	int[] marks;
	int roll_no;
	
	public void display() {
		System.out.println(name+"\t"+marks+"\t"+roll_no);
		
	}

public Student() {
	name="sakshi";
	marks[]={78,98,87};
	roll_no=34;
	
}

public Student(String names, int[] score,int rollno) {
	this.name=names;
	this.marks=score;
	this.roll_no=rollno;
	
} {
	
}





}
